import win.ui;
/*DSG{{*/
var winform = win.form(text="Web Form";right=599;bottom=399;bgcolor=16777215)
winform.add()
/*}}*/

// 创建web窗体
import web.form; 
var wb = web.form( winform,,,,true);
wb.noScriptErr = true;

winform.openUrl = function(url){
	wb.go(url) 
}

// 打开目标网站
winform.show(); // 显示窗体
 
// 启动消息循环
win.loopMessage(); 
return  winform; 
