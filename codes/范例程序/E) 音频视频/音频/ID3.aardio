//ID3标签
import console; 
import fsys.tagLib;
import fsys.dlg;

var mp3Path = fsys.dlg.open("*.mp3|*.mp3");
if(!mp3Path) return;

var tagFile = fsys.tagLib(mp3Path);
console.dumpJson(tagFile);

for name,value in tagFile.each(){
	console.log(name,value);
}

console.log(tagFile.title);
console.pause(true);
