//main 函数
import console;
import tcc; 

var c = tcc(); 
c.enableIoPrintf();

c.code = /**
	#include <stdio.h> 
	
	int main (int argc, char *argv[])     
	{   
    	int i = 0; 
    	for( i=1;i<argc;i++){
    		io_printf( "%s \n" , argv[i] );
    	}
     	
    	return 0; 
	}
**/
 
//直接运行 main() 入口函数
var ret = c.run("測試命令參數","測試命令參數2","支持任意多個參數");
console.print( ret )

c.close(); 
console.pause();