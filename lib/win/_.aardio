setenv =  ..string.setenv; //@Deprecated
getenv = ..string.getenv; //@Deprecated
addenv = function(k,nv,path){ //@Deprecated
	if(path) nv = ..io.fullpath(nv);
	var values = ..string.getenv(k);  
	if( !#values ){
		..string.setenv(k,nv)
		return nv;
	}
	
	var tv = ..string.split(values,"<;>");
	for(k,v in tv){
		if( ..string.cmp(nv,v) == 0 ) return values; 
	}
	
	values = nv+ ";" + values;
	..string.setenv(k,values)
	return values;
} 

