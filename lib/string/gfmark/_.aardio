
namespace string.gfmark{
   
	_dll = ..raw.loadDll("~\lib\string\gfmark\.res\cmark-gfm.dll",,"cdecl");
	
	render = function(md,opt){
		var pHtml = _dll.cmark_markdown_to_htmlP(md,#md,opt || 0/*CMARK_OPT_DEFAULT*/)
		return pHtml  ? ..raw.str(pHtml)
	} 
}

