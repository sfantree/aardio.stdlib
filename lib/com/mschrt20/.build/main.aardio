if( ! io.exist(pubOcxDir) ){ 
	var libPath,libDir = io.libpath("com.mschrt20");
	..fsys.copy( fsys.joinpath(libDir,"\.res\*.*") ,pubOcxDir,0x10/*_FOF_NOCONFIRMATION*/  | 0x200/*_FOF_NOCONFIRMMKDIR*/);
 
}
